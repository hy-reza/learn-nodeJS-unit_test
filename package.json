{
  "name": "learn-unit-test-nodejs",
  "version": "1.0.0",
  "description": "belajar unit test ",
  "main": "./src/app.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "maxConcurrency": 1
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hy-reza/learn-unit-test-nodeJS.git"
  },
  "keywords": [
    "unit testing",
    "nodeJS",
    "JestJS",
    "Javascript"
  ],
  "author": "hy-reza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hy-reza/learn-unit-test-nodeJS/issues"
  },
  "homepage": "https://github.com/hy-reza/learn-unit-test-nodeJS#readme",
  "devDependencies": {
    "babel-jest": "^29.0.3",
    "jest": "^29.0.3"
  }
}
